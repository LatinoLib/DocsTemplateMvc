<!DOCTYPE html> 
<html lang=en>
   <head>
      <meta charset=utf-8>
      <meta http-equiv=X-UA-Compatible content="IE=edge">
      <meta name=viewport content="width=device-width,initial-scale=1">
      <meta name=description content="@ViewBag.ProjectDescription">
      <meta name=keywords content="@ViewBag.ProjectKeywords">
      <meta name=author content="@ViewBag.ProjectAuthors">
      <title> @ViewBag.PageTitle </title>
      @Styles.Render("~/css/bootstrap-docs")
      @Styles.Render("~/css/prism")
      @Styles.Render("~/css/custom")
      <link rel=apple-touch-icon href='~/images/apple-touch-icon.png'>
      <link rel=icon href='~/images/favicon.ico'>
   </head>
   <body>
      <a id=skippy class="sr-only sr-only-focusable" href=#content>
         <div class=container><span class=skiplink-text>Skip to main content</span></div>
      </a>
      <header class="navbar navbar-static-top bs-docs-nav" id=top role=banner>
         <div class=container>
            <div class=navbar-header> <button class="navbar-toggle collapsed" type=button data-toggle=collapse data-target=#bs-navbar aria-controls=bs-navbar aria-expanded=false> <span class=sr-only>Toggle navigation</span> <span class=icon-bar></span> <span class=icon-bar></span> <span class=icon-bar></span> </button> <a href='./' class=navbar-brand>@Html.Raw(ViewBag.ProjectName)</a> </div>
            @RenderPage("TopNav.cshtml")
         </div>
      </header>
      <div class=bs-docs-header id=content tabindex=-1>
         <div class=container>
            <h1>@Html.Raw(ViewBag.PageTitle)</h1>
            <p>@Html.Raw(ViewBag.PageDescription)</p>
         </div>
      </div>
      <div class="container bs-docs-container">
         <div class=row>
            <div class=col-md-@(IsSectionDefined("SideNav") ? "9" : "12") role=main>
               <div class=bs-docs-section>
                  @RenderBody()
               </div>
            </div>
            @if (IsSectionDefined("SideNav"))
            {
            <div class=col-md-3 role=complementary>
               <nav class="bs-docs-sidebar hidden-print hidden-xs hidden-sm">
                  <ul class="nav bs-docs-sidenav">
                    @RenderSection("SideNav", required: false)
                  </ul>
                  <a class=back-to-top href=#top> Back to top </a> 
               </nav>
            </div>
            }
         </div>
      </div>
      <footer class=bs-docs-footer role=contentinfo>
         <div class=container>
            @RenderPage("FooterMenu.cshtml")
            @RenderPage("FooterText.cshtml")
         </div>
      </footer>
      <script type="text/javascript">var __root__ = "@Url.Content("~")";</script>
      @Scripts.Render("~/js/jquery")
      @Scripts.Render("~/js/bootstrap-docs")
      @Scripts.Render("~/js/prism")
      @Scripts.Render("~/js/hacks")
      <script src="~/MathJax/MathJax.js?config=TeX-AMS_SVG"></script>
      @Scripts.Render("~/js/custom")
   </body>
</html>
<!DOCTYPE html> 
<html lang=en>
   <head>
      <meta charset=utf-8>
      <meta http-equiv=X-UA-Compatible content="IE=edge">
      <meta name=viewport content="width=device-width,initial-scale=1">
      <meta name=description content="@ViewBag.Description">
      <meta name=keywords content="@ViewBag.Keywords">
      <meta name=author content="@ViewBag.Authors">
      <title> @ViewBag.Title </title>
      @Styles.Render("~/css/bootstrap-docs")
      @Styles.Render("~/css/prism")
      @Styles.Render("~/css/custom")
      <link rel=apple-touch-icon href='~/images/apple-touch-icon.png'>
      <link rel=icon href='~/images/favicon.ico'>
   </head>
   <body>
      <a id=skippy class="sr-only sr-only-focusable" href=#content>
         <div class=container><span class=skiplink-text>Skip to main content</span></div>
      </a>
      <header class="navbar navbar-static-top bs-docs-nav" id=top role=banner>
         <div class=container>
            <div class=navbar-header> <button class="navbar-toggle collapsed" type=button data-toggle=collapse data-target=#bs-navbar aria-controls=bs-navbar aria-expanded=false> <span class=sr-only>Toggle navigation</span> <span class=icon-bar></span> <span class=icon-bar></span> <span class=icon-bar></span> </button> <a href='./' class=navbar-brand>@ViewBag.ProjectName</a> </div>
            <nav id=bs-navbar class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li class=active> <a href="#">Left Link 1</a> </li>
                    <li> <a href="#">Left Link 2</a> </li>
                    <li> <a href="#">Left Link 3</a> </li>
                    <li> <a href="#">Left Link 4</a> </li>
                    <li> <a href="#">Left Link 5</a> </li>
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li><a href='#'>Right Link 1</a></li>
                    <li><a href='#'>Right Link 2</a></li>
                </ul>
            </nav>
         </div>
      </header>
      <div class=bs-docs-header id=content tabindex=-1>
         <div class=container>
            <h1>@ViewBag.Title</h1>
            <p>@ViewBag.Description</p>
         </div>
      </div>
      <div class="container bs-docs-container">
         <div class=row>
            <div class=col-md-@(IsSectionDefined("SideNav") ? "9" : "12") role=main>
               <div class=bs-docs-section>
                  @RenderBody()
               </div>
            </div>
            @if (IsSectionDefined("SideNav"))
            {
            <div class=col-md-3 role=complementary>
               <nav class="bs-docs-sidebar hidden-print hidden-xs hidden-sm">
                  <ul class="nav bs-docs-sidenav">
                    @RenderSection("SideNav", required: false)
                  </ul>
                  <a class=back-to-top href=#top> Back to top </a> 
               </nav>
            </div>
            }
         </div>
      </div>
      <footer class=bs-docs-footer role=contentinfo>
         <div class=container>
            <ul class=bs-docs-footer-links>
               <li><a href='#'>Link 1</a></li>
               <li><a href='#'>Link 2</a></li>
               <li><a href='#'>Link 3</a></li>
               <li><a href='#'>Link 4</a></li>
            </ul>
            <p>Made with love by some cool guys.</p>
            <p>Some copyright text.</p>
         </div>
      </footer>
      @Scripts.Render("~/js/jquery")
      @Scripts.Render("~/js/bootstrap-docs")
      @Scripts.Render("~/js/prism")
      @Scripts.Render("~/js/hacks")
   </body>
</html>